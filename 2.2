import math

eps = 1e-10
g = 9.8

def f(b, v, a):
    b_rad = math.radians(b)
    return a - (v**2 * math.sin(2 * b_rad)) / g

def bin_solve(b, v):
    left, right = 0.0, 10000.0
    while right - left > eps:
        mid = (right + left) / 2.0
        if f(b, v, mid) * f(b, v, left) < 0:
            right = mid
        else:
            left = mid
    return (left + right) / 2.0

def main():
    print("Root:", bin_solve(45.0, 10.0))

if __name__ == "__main__":
    main()
