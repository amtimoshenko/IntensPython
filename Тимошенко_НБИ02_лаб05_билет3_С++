#include <iostream>
using namespace std;

int main() {
    int a;
    cin >> a;
    cout<< "a=";
    while(a%2==0)
    {
		a=a/2;
        cout << "2*";
       if(a%3==0)
       {
           a=a/3;
           cout << "3*";
       }
       if(a%5==0)
       {
           a=a/5;
           cout << "5*";
       }
       if(a%7==0)
       {
           a=a/7;
           cout << "7*";
       }
       if(a%11==0)
       {
           a=a/11;
           cout << "11*";
       }
       cout << "1";
    }
    while(a!=1)
    {
       if(a%3==0)
       {
           a=a/3;
           cout << "3*";
       }
       if(a%5==0)
       {
           a=a/5;
           cout << "5*";
       }
       if(a%7==0)
       {
           a=a/7;
           cout << "7*";
       }
       if(a%11==0)
       {
           a=a/11;
           cout << "11*";
       }
       if(a==1)
       {
		   cout << "1";
	   }
    }
   
    return 0;
}
