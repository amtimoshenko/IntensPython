#include <iostream>
#include <cmath>
#include <limits>

double calculateSum(double x, int maxIterations) {
    double sum = 0.0;
    for (int n = 1; n <= maxIterations; ++n) {
        sum += (x * x) / (1 + std::pow(n, 3) * x);
    }
    return sum;
}

int main() {
    double x;
    int maxIterations;

    std::cout << "Vvedite x: ";
    std::cin >> x;
    std::cout << "Vvedite maxIterations: ";
    std::cin >> maxIterations;

    double result = calculateSum(x, maxIterations);
    std::cout << "Summa: " << result << std::endl;

    return 0;
}
