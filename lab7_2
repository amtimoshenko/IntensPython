N=int(input("Введите количество элеиентов массива:"))
x=[0]*N
print("Введите элементы массива:")
for i in range(N):
    x[i]=int(input())
k=int(input("Введите число, на которое сдвигаем элементы массива:"))
l=len(x)
a=[0]*l
for i in range(l):
    a[i]=x[(i+k)%l]
print("Массив после сдвига на", k, "элементов влево: ", a)
